Program
  Vars
    x Real
    y Int
    b Bool
    ar array[10] of Real
  EndVars

  Function P(val p Int) Return Real
    Return ar[1]
  EndFunction
  x := |int| 3
  x := |int| 3.14 + 2
  x := |real| b + 2.4
  x := |real| y + 2.4
  y := |int| Floor ar[7] + 2
  y := 3 * |int| ar[|int| 6.45] + P(|real| 45)
  y := 3 * |int| ar[|int| 6.45] +  |int| P(|int| 45.3)

EndProgram
